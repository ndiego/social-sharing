{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "outermost/social-sharing-link",
  "title": "Social Sharing Link",
  "parent": [
    "outermost/social-sharing"
  ],
  "description": "Display an icon that shares the current page when clicked.",
  "textdomain": "social-sharing",
  "attributes": {
    "service": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    }
  },
  "usesContext": [
    "showLabels",
    "iconColorValue",
    "iconBackgroundColorValue"
  ],
  "supports": {
    "reusable": false,
    "html": false
  },
  "editorScript": "outermost-social-sharing-scripts",
  "editorStyle": "file:./../../editor.css"
}